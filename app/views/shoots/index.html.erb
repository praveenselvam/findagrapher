<h1>Listing shoots</h1>

<table>
  <tr>
    <th>Name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @shoots.each do |shoot| %>
  <tr>
    <td><%= shoot.name %></td>
    <td><%= link_to 'Show', shoot %></td>
    <td><%= link_to 'Edit', edit_shoot_path(shoot) %></td>
    <td><%= link_to 'Destroy', shoot, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Shoot', new_shoot_path %>
